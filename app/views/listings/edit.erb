<h1>Edit listing</h1>
<div class="listing-form">
<%= form_for @listing do |f| %>
        <%=f.label :title %> <br>
        <%= f.text_field :title %> <br> <br>
        <%= f.label :body %> <br>
        <%= f.text_area :body %> <br> <br>
        <%= f.label :category %> <br>
        <%=f.collection_select(:category_id, Category.all, :id, :name) %> <br> <br>
  <%= f.submit %>
<% end %>
</div>
