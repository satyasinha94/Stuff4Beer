<h1>Create A New Listing</h1>
<%= form_for @listing do |f| %>
  Title:<%= f.text_field :title %> <br> <br>
  Body: <%= f.text_area :body %> <br> <br>
  Category: <%=f.collection_select(:category_id, Category.all, :id, :name) %> <br> <br>
  User: <%=f.collection_select(:user_id, User.all, :id, :name) %> <br> <br>
  <%= f.submit %>
<% end %>
