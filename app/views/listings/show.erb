<h1><%=@listing.title %></h1>
 <p>Description: <%= @listing.body %></p>
 <p>Category: <%= @listing.find_category %></p>
 <p>User: <%= @listing.find_user.name %></p>
 <p>Contact Info: <%= @listing.find_user.email %></p>


 <% if current_user == @listing.user %>
 <%= link_to "Edit Listing", edit_listing_path %> <br> <br>
 <% end %>

 <% if current_user == @listing.user %>
 <%= button_to "Delete Listing", listing_path(@listing), method: :delete %> <br> <br>
 <% end %>
